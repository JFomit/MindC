void print_number(number x)
{
	mlog
	{
		print $x;
	}
}
void printflush()
{
	mlog
	{
		printflush message1;
	}
}
number random()
{
	number result;
	mlog
	{
		op rand $result 1;
	}

	return result;
}

void main()
{
	number x;
	x = random();

	if (x < 0.5)
	{
		print_number(x);
	}
	print_number(-1);
	printflush();
}
